{
  "name": "ng-terminal-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ng build && ng serve",
    "build": "npm run copy-readme && ng build",
    "lib-build": "npm run copy-readme && ng build ng-terminal",
    "travis-test": "ng test ng-terminal --watch false --browsers ChromeHeadless",
    "lib-test": "ng test ng-terminal",
    "gh-pages": "npm run lib-build && ng build --prod --output-path docs --base-href /ng-terminal/",
    "copy-readme": "copyfiles README.md projects/ng-terminal/",
    "patch-up": "cd projects/ng-terminal/ && npm version patch",
    "minor-up": "cd projects/ng-terminal/ && npm version minor",
    "major-up": "cd projects/ng-terminal/ && npm version major",
    "publish": "cd dist/ng-terminal && npm publish",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "add-licenses": "license-checker > LICENSE-THIRD-PARTY"
  },
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/forms": "~10.2.4",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "angular-resizable-element": "^3.3.4",
    "copyfiles": "^2.4.1",
    "core-js": "^2.6.12",
    "ngx-json-viewer": "^2.4.0",
    "rxjs": "6.5.5",
    "tslib": "^2.0.0",
    "xterm": "^4.10.0",
    "xterm-addon-fit": "^0.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular-devkit/build-ng-packagr": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/language-service": "~10.2.4",
    "@types/jasmine": "~2.8.17",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "license-checker": "^25.0.1",
    "ng-packagr": "^10.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
